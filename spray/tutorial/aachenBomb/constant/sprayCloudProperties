{
    // thermal shielding
    BirdCorrection  true;
}

SinglePhaseMixtureCoeffs
{
    phases
    (
        liquid
        {
            C7H16 1;
        }
    );
}

LiquidEvaporationCoeffs
{
    enthalpyTransfer enthalpyDifference;

    activeLiquids
    (
        C7H16
    );
}

ConeInjectionCoeffs
{
    SOI               0.0;
    massTotal         massTotal [1 0 0 0 0 0 0] 6.0e-6;
    parcelBasisType   mass;
    duration          1.25e-3;
    position          ( 0 0.0995 0 );
    direction         ( 0 -1 0 );
    parcelsPerSecond  10000000;
    volumeFlowRate    constant 1.0;
    Umag              constant 300.0;

    thetaInner        constant 0.0;
    thetaOuter        constant 20.0;

    parcelPDF
    {
        pdfType       uniform;

        uniformPDF
        {
            minValue  1.0e-4;
            maxValue  1.1e-4;
        }

    }

}

AtomizationModel LISA;

BreakupModel     none;

